{
  "manifest_version": 3,
  "name": "Windows Event Lookup",
  "description": "Lookup Windows Event IDs on Ultimate Windows Security",
  "version": "1.0",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon48.png",
    "default_title": "Lookup Windows Event"
  },
  "host_permissions": [
    "*://www.ultimatewindowssecurity.com/*"
  ]
}
